    <Style TargetType="Button" x:Key="SchemaSeqButton" BasedOn="{x:Null}">
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="FontFamily" Value="{StaticResource SchemaSeqIconfont}"/>
